{
	"npmScope": "vscode-devkit",
	"affected": {
		"defaultBase": "master"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/workspace/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"]
			}
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	},
	"projects": {
		"nx": {
			"tags": []
		},
		"nx-e2e": {
			"tags": [],
			"implicitDependencies": ["nx"]
		}
	},
	"workspaceLayout": {
		"appsDir": "e2e",
		"libsDir": "packages"
	}
}
